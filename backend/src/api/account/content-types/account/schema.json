{
  "kind": "collectionType",
  "collectionName": "accounts",
  "info": {
    "singularName": "account",
    "pluralName": "accounts",
    "displayName": "account",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "email": {
