    },
    "sessions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::session.session",
      "mappedBy": "accounts"
    },
    "password": {
      "type": "string"
    },
    "avatar": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
